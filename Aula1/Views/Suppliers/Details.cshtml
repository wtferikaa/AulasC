@model Aula1.Controllers.Supplier

@{
    Layout = "~/Views/Shared/_Layout.cshtml";
    ViewBag.Title = "Visualizando	detalhes	de	um	suppliers";

}

<div class="panel	panel-primary">
    <div class="panel-heading">
        Visualizando	detalhes	de	um	supplier
    </div>
    <div class="panel-body">
        <div class="form-group">
            @Html.LabelFor(model => model.Name)
            <br />
            <div class="input-group">
                <span class="input-group-addon">
                    <i class="glyphicon	glyphicon-user"></i>
                </span>
                @Html.EditorFor(model => model.Name, new { htmlAttributes = new { @class = "form-control", disabled = "disabled" } })
            </div>
        </div>
    </div>
    <div class="panel-footer	panel-info">
        @Html.ActionLink("Alterar", "Edit", new { id = Model.SupplierId }, new { @class = "btn	btn-info" })
        @Html.ActionLink("Retornar	para	a	listagem", "Index", null, new { @class = "btn	btn-info" })
    </div>
</div>
